{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "eslint-config-airbnb-base",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "globals": {
    "jQuery": "readonly",
    "Drupal": "writable",
    "drupalSettings": "readonly"
  },
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "import/extensions": 0,
    "prettier/prettier": "error"
  }
}
