{#
  /**
  * Frontpage layout
  *
  * Example page for the document structure of the Harbor Theme
  * This template uses the general page layout.
  * Documentation of the availlable functions can be found at: ../default/page.html.twig
  *
#}


{% extends "@harbor/src/pages/default/page.html.twig" %}

{% block document_head %}
  {% include "@harbor/src/utilities/loadcss.twig" with {
    cwd: theme_path,
    stylesheets: [
      '/templates/dist/pages/front/stylesheets/index.css'
    ]
  } %}
{% endblock %}

{% block content_before %}
  hallo wereld
{% endblock %}

{% block document_after %}
  {% include "@harbor/src/utilities/inline-svg-sprite-injector.twig" with {
    svg_sprite: theme_path ~ '/templates/dist/main/images/svg-sprite.svg',
  } %}
{% endblock %}