{
  "name": "@toolbarthomas/harbor",
  "version": "0.116.0",
  "type": "module",
  "description": "Boilerplate for Drupal Themes",
  "exports": "./Harbor/index.js",
  "bin": {
    "harbor": "./index.js"
  },
  "node": "^12.22.0 || ^14.17.0 || >=16.0.0",
  "scripts": {
    "production": "node ./index.js --task=prepare,compile --minify",
    "predevelopment": "npm run production",
    "development": "node ./index.js --watch --styleguide",
    "images": "node ./index.js --task=images",
    "javascripts": "node ./index.js --task=javascripts",
    "resolve": "node ./index.js --task=resolve",
    "serve": "node ./index.js --serve",
    "styleguide": "node ./index.js --styleguide",
    "stylesheets": "node ./index.js --task=stylesheets",
    "setup": "node ./index.js --task=setup",
    "test": "node ./index.js --task=test",
    "test:reference": "node ./index.js --test=reference",
    "test:approve": "node ./index.js --test=approve"
  },
  "files": [
    ".babelrc",
    ".browserlistrc",
    ".eslintignore",
    ".eslintrc",
    ".prettierrc",
    ".stylelintrc",
    "./index.js",
    "Harbor/",
    ".storybook/index.ejs",
    "harbor.config.js",
    "harbor.default.config.js",
    ".env"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toolbarthomas/harbor.git"
  },
  "keywords": [
    "Drupal",
    "Theme"
  ],
  "author": "Thomas van der Velde <contact@toolbarthomas.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/toolbarthomas/harbor/issues"
  },
  "homepage": "https://github.com/toolbarthomas/harbor#readme",
  "dependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.7",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/cli": "^6.4.19",
    "@storybook/html": "^6.4.19",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "backstopjs": "^6.0.4",
    "camelcase": "^6.3.0",
    "chalk": "^5.0.1",
    "chokidar": "^3.5.3",
    "concat": "^1.0.3",
    "copyfiles": "^2.4.1",
    "cssnano": "^5.1.4",
    "dotenv": "16.0.0",
    "eslint": "^8.11.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "html-escaper": "^3.0.3",
    "imagemin": "^8.0.1",
    "is-svg": "^4.3.2",
    "log-symbols": "^5.1.0",
    "mkdirp": "^1.0.4",
    "node-sass-glob-importer": "^5.3.2",
    "outdent": "^0.8.0",
    "postcss": "^8.4.8",
    "postcss-combine-duplicated-selectors": "^10.0.3",
    "postcss-scss": "^4.0.3",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "sass": "^1.49.9",
    "snake-case": "^3.0.4",
    "stringify-object": "^4.0.1",
    "stylelint": "^14.5.3",
    "stylelint-config-recommended-scss": "^5.0.2",
    "stylelint-scss": "^4.1.0",
    "svgo": "^2.8.0",
    "svgstore": "^3.0.1",
    "twing": "^5.1.0",
    "twing-loader": "^4.0.0",
    "type-name": "^2.0.2",
    "uglify-js": "^3.15.3",
    "ws": "^8.5.0",
    "yaml-loader": "^0.6.0"
  }
}
